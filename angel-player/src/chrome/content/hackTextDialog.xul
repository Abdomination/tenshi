<?xml version="1.0"?>

<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>

<dialog id="hackTextDialog" title="Input text"
    xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
    buttons="accept"
    ondialogaccept="window.arguments[0](document.getElementById('mainbox').value, document.getElementById('containercheck').checked)">

    <textbox id="mainbox"/>
    <checkbox label="Container?" id="containercheck"/>
</dialog>
